(executable
 ((name rewrite)
  (modules rewrite)
  (libraries (bigarray unix xapi-rrd xapi-rrd-unix))
 ))

(executable
 ((name unit_tests)
  (modules (unit_tests))
  (libraries (bigarray oUnit unix xapi-rrd xapi-rrd-unix))
 ))

(alias
 ((name    runtest)
  (package xapi-rrd-unix)
  (deps    (unit_tests.exe rewrite.exe))
  (action  (run ${<}))))
