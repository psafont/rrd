OCAML_LIBS+=../util/encodings ../idl/ocaml_backend/common ../idl/ocaml_backend/client /opt/xensource/lib/ocaml/toplevellib
OCAMLINCLUDES +=  ../idl/ocaml_backend/ ../autogen/ ../idl ../muxdemux ../xva ../util ../http 
