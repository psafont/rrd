section
	OCAMLFLAGS+=-linkall
	OCamlProgram(xencaml-bin, toplevelhelper /opt/xensource/lib/ocaml/topstart )

.PHONY: clean

clean:
	rm -f *.cmi *.cmx *.cmo *.a *.cma *.cmxa *.run *.opt *.annot *.o

xencaml: xencaml-bin.run
	./mkwrapper
